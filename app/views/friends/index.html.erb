<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<h1 id="title">All Your Friends</h1>
		<%= will_paginate %>
		<% if @friends.any? %>
			<%= render partial: "all_friends", locals: {friends: @friends}  %>
		<% else %>
			<p>No friends to show.</p>
		<% end %>
	</div>
</div>
<%= will_paginate %>