<% friend = @friend_request.friend %>
$('#remove_friend_request_<%= friend.id %>').html("<%= escape_javascript(render partial: 'users/shared/add_friend_link', locals: {user: friend})%>");
$(".notice").html("Friend Request deleted.").show();
$("#flash_notice").show();
$('#remove_<%=friend.id%>').remove();
$('#pending').html("<%= escape_javascript(render partial: "layouts/count_pending_friends")%>");