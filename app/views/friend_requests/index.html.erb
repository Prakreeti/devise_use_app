<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<div id="flash_notice"><div class="notice"></div></div>
		<h1 id="title">Pending Requests</h1>
		<% if @incoming.any? %>
			<%= render partial: "friend_requests", locals: {incoming: @incoming}  %>
		<% else %>
			<p>There are no pending friend requests.</p>
		<% end %>
	</div>
</div>

